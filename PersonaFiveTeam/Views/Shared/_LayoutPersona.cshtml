                        <li class="scroll"><a asp-area="" asp-controller="Home" asp-action="Confidants">Confidants</a></li>
                        <li class="scroll"><a asp-area="" asp-controller="Home" asp-action="Skills">Skills</a></li>
                        <li class="scroll"><a asp-area="" asp-controller="Home" asp-action="Mementos">Mementos</a></li>
